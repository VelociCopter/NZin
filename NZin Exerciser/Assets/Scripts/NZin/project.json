{
  "name": "NZinUnitTests",
  "displayName": "NZinUnitTests",

  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true
  },

  "frameworks": {

    "UnityEngine": {
        "bin": {
            "assembly": "lib/UnityEngine.dll"
        }
    },
    

    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      },
      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  },


  "dependencies": {
      "NUnit": "3.4.0",
      "dotnet-test-nunit": "3.4.0-beta-3"
  },
  
  "testRunner": "nunit"

}